    "connect-timeout": "1.7.0",
    "express-http-proxy": "^0.6.0",
    "moment": "^2.13.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.4.0",
    "react-redux": "^4.1.2",
    "react-router": "^2.2.2",
    "react-router-redux": "^4.0.2",
    "redux": "^3.1.2",
    "redux-logger": "^2.4.0",
